    "@types/luxon": "^2.0.3",
    "@types/node": "^15.12.4",
    "@types/react": "^18.2.43",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^18.2.17",
    "@types/react-phone-number-input": "^3.0.12",
    "@types/react-scroll": "^1.8.2",
    "@types/react-select": "^4.0.17",
    "@types/react-slick": "^0.23.12",
    "@types/react-stonecutter": "^0.3.1",
